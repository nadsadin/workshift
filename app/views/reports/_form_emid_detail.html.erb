<%= form_for @report, :url => {:action => "employee_id_detail"}, :method => "get" do |f| %>
    <div class="form-group">
      <%= f.collection_select :employee_id, Employee.all, :id, :name, {}, {class: "form-control col-md-5"} %>
    </div>
    <div class="form-group row">
      <%= f.label :start_date, 'Start', class: "col-md-1" %>
      <%= f.date_select :start_date,with_css_classes: {year: "col-md-1", month: "col-md-2", day: "col-md-1"} %>

      <%= f.label :end_date, 'End', class: "col-md-1" %>
      <%= f.date_select :end_date, with_css_classes: {year: "col-md-1", month: "col-md-2", day: "col-md-1"} %>
    </div>


    <%= f.submit 'Search' %>
<% end %>